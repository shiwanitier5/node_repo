{
   "manifest_version": 2,
   "name": "GeniusMail: Powerful mail merge for Gmail",
   "short_name": "GeniusMail",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0",
   "content_scripts": [ {
    
      "js": [ "inboxsdk.js", "geniusmail.js", "jquery.js", "jquery.bpopup.min.js", "select2.full.js", "selectize.js" ],
      "matches": [ "http://mail.google.com/*", "https://mail.google.com/*", "https://inbox.google.com/*", "http://inbox.google.com/*" ],
      "run_at": "document_end"
   } ],
   "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
   
   "description": "A powerful mass email and mail merge system for Gmail.",
  
   "key": "",
  
   "oauth2": {
      "client_id": "926769641775-b1905ddt8ofmt3qiq46btskdr0r36dm3.apps.googleusercontent.com",
      "scopes": [
          "https://www.googleapis.com/auth/userinfo.email",
          "https://www.googleapis.com/auth/userinfo.profile",
          "https://mail.google.com/",
          "https://www.googleapis.com/auth/spreadsheets",
          "https://www.googleapis.com/auth/drive.file"
      ]
     
  },
   "permissions": [ 
       "http://www.gmass.co/",
       "identity",
       "tabs",
       "storage" ]
   
  
}
